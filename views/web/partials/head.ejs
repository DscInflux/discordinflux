  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.min.js"></script>
  <script>
   $(document).ready(function() {
      // Load the text color from the cookie and set it as the default color
      var defaultTextColor = getTextColorCookie();
      if (defaultTextColor) {
        $('#change-text-color-display').css('color', defaultTextColor);
      }
      
      // Handle the color change when the dropdown is clicked
      $('.dropdown-menu a').click(function() {
  var newColor = $(this).attr('id');
  changeTextColor(newColor);
});


    function changeTextColor(color) {
      // Apply the color to the text
      $('#change-text-color-display').css('color', color);
      
      // Get the current text color of the element and set the cookie
      var textColor = $('#change-text-color-display').css('color');
      setTextColorCookie(textColor);
    }

    function setTextColorCookie(color) {
      // Set a cookie with the text color that expires in 30 days
      var expiryDate = new Date();
      expiryDate.setDate(expiryDate.getDate() + 30);
      var cookieValue = encodeURIComponent(color) + '; expires=' + expiryDate.toUTCString() + '; domain=' + window.location.hostname + '; path=/';
      document.cookie = 'textColor=' + cookieValue;
    }

    function getTextColorCookie() {
      // Get the value of the textColor cookie, if it exists
      var cookieName = 'textColor=';
      var cookieArray = document.cookie.split(';');
      for (var i = 0; i < cookieArray.length; i++) {
        var cookie = cookieArray[i];
        while (cookie.charAt(0) == ' ') {
          cookie = cookie.substring(1);
        }
        if (cookie.indexOf(cookieName) == 0) {
          return decodeURIComponent(cookie.substring(cookieName.length, cookie.length));
        }
      }
      return null;
    }

  </script>
