<!DOCTYPE html>
<html>
  <head>
    <title>Partners</title>
    <%- include('partials/head'); -%>
  </head>
  <body class="d-flex flex-column min-vh-100">
    <%- include('partials/loader'); -%>

    <%- include('partials/navbar'); -%>
    <div class="container mt-5">
      <div class="row text-center mt-5">
        <div class="ds">
          <h2 class="mb-1"><i class="fad fa-handshake-alt" style="--fa-primary-color: #ffffff; --fa-secondary-color: #222;"></i> Partners</h2>
          <h5 class="text-damp">Discover the Incredible Partnerships of DiscordInflux - Join the Community Today!</h5>
        </div>
      </div>
      <!-- Top Users Section -->
      <div class="row justify-content-center mt-5">
        <div class="row row-cols-1 row-cols-md-2 g-4">
          <% partners.forEach(partner => { %>
            <div class="col">
              <div class="card rounded-5" style="background-color: #111; color: white;">
                <!-- <img src="<%= partner.banner ? partner.banner : 'https://mediashare.ink/file/1683510439225.png' %>" style="width: 100%; height: 190; border-top-left-radius: 10px; border-top-right-radius: 10px;" class="card-img-top" alt="Partners"> -->
                <div class="card-body">
                  <h5 class="card-title"><img style="background-color: #111;" src="<%= partner.logoUrl %>" width="50" height="50" class="rounded-5" alt="Partners"> <%= partner.name %></h5>
                  <p class="card-text">
                    <% if (partner.description) { %>
                      <%= partner.description %>
                    <% } else { %>
                      <p>There Was An Error Or The Staff Didn't Add a Headline!</p>
                    <% } %>
                  </p>
                  <a href="<%= partner.websiteUrl %>?referrer=DiscordInflux" class="btn text-center" target="_blank" style="background-color: #222; color: white;">Website</a>
                </div>
              </div>
            </div>
          <% }); %>
        </div>
      </div>
    </div>

    <%- include('partials/footer'); -%>

   
  </body>
</html>
